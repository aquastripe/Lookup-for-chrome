{
	"name": "Lookup Definitions Online",
	"version": "1.3.4",
	"manifest_version": 2,
	
	"background": { "page" : "/html/background.html" },
	"description": "Look up definitions from free online sources.",
	"icons": {
	  "48": "/images/mybug.gif"
	},
	"minimum_chrome_version": "8",
	"permissions": [
		"contextMenus",
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"options_page": "/html/options.html",
	"update_url": "http://clients2.google.com/service/update2/crx",
	
	"browser_action": {
		"default_icon": "images/mybug.gif",
		"default_title": "Lookup Defintions Online",
		"default_popup": "/html/options.html"
	}
}
